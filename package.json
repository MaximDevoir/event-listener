  "author": {
    "name": "Maxim Devoir",
    "email": "maximdevoir@hotmail.com"
  },
  "scripts": {
    "build": "webpack -p --progress --colors",
    "build:clean": "rm -rf ./dist/* && rm -rf ./docs/* && rm -rf ./coverage/*",
    "build:codecov": "./node_modules/.bin/babel-node ./node_modules/istanbul/lib/cli cover ./node_modules/mocha/bin/_mocha -- ./tests/*.spec.js",
    "build:docs": "jsdoc -c .jsdoc.json",
    "lint": "eslint --ignore-path .eslintignore .",
    "prebuild": "npm run build:clean && npm run test",
    "postbuild": "npm run build:codecov && npm run build:docs",
    "postinstall": "node ./scripts/postinstall.js",
    "pretest": "npm run lint",
    "start": "webpack --watch --progress --colors",
    "test": "mocha --compilers js:babel-register tests/*.spec.js"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "^6.4.1",
    "expect.js": "^0.3.1",
    "istanbul": "^1.0.0-alpha.2",
    "jsdoc": "^3.4.2",
    "minami": "^1.1.1",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.2"
  }
}
